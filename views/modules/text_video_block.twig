<div class="text-image-block video {{module.layout}} {{module.background}}">
	<div class="container">
		<div class="row">
			<div class="video">
				{{module.video_embed}}
			</div>
			<div class="text">
				<h2>{{module.heading}}</h2>
				{{module.text}}
				{% if module.cta %}
					<a href="{{module.cta.url}}" target="{{module.cta.target}}" class="btn">
						{{module.cta.title}}
						<i class="fas fa-angle-double-right"></i>

					</a>
				{% endif %}
			</div>
		</div>
	</div>
</div>
