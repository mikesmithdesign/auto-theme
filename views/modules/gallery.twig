<div class="gallery-wrapper {{module.background}}">
	<div class="container">
		<div class="row">
			{% if module.heading %}
				<h2>{{module.heading}}</h2>
			{% endif %}


			<div class="gallery {{module.layout}}">
				{% for image in module.images %}
					<a data-fancybox="gallery-{{module_index}}" href="{{get_image(image).src}}">
						<img src="{{get_image(image).src}}" alt="{{get_image(image).alt}}">
					</a>
				{% endfor %}
			</div>
		</div>
	</div>
</div>
